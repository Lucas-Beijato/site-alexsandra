---
import type { ImagesUrls } from "../../assets/types"

const urlsLogo: ImagesUrls = {
  avif: "/pictures/base/logo.avif",
  webp: "/pictures/base/logo.webp",
  jpg: "/pictures/base/logo.jpg"
}
---

<picture>
    <source srcset={urlsLogo.webp} type="image/webp">
    <source srcset={urlsLogo.avif} type="image/avif">
    <img width="100" height="64" class="rounded-md" src={urlsLogo.jpg} alt="Foto Alexsandra">
</picture>